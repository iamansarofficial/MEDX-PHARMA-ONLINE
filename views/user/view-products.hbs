<script>
  $(document).ready(function() {
    // Category filter click event
    $('.btn-group-vertical .btn').click(function(e) {
      e.preventDefault();
      var category = $(this).data('category');

      // Remove active class from all buttons and add it to the clicked button
      $('.btn-group-vertical .btn').removeClass('active');
      $(this).addClass('active');

      // Show/hide products based on the selected category
      if (category === 'all') {
        $('.product-item').show();
      } else {
        $('.product-item').hide();
        $('.product-item[data-category="' + category + '"]').show();
      }
    });
  });
</script>





<script>
  document.addEventListener('DOMContentLoaded', function() {
    // Get all the checkbox inputs
    var checkboxes = document.querySelectorAll('.filter-price-options input[type="checkbox"]');

    // Add click event listener to each checkbox
    checkboxes.forEach(function(checkbox) {
      checkbox.addEventListener('click', function() {
        handleCheckboxClick();
      });
    });

    // Function to handle checkbox click event
    function handleCheckboxClick() {
      // Get the selected price range values
      var selectedRanges = [];
      checkboxes.forEach(function(checkbox) {
        if (checkbox.checked) {
          selectedRanges.push(checkbox.value);
        }
      });

      // Show or hide product items based on the selected price ranges
      var productItems = document.querySelectorAll('.product');
      productItems.forEach(function(item) {
        var itemPrice = parseFloat(item.getAttribute('data-price'));
        var shouldShow = false;

        if (selectedRanges.length === 0) {
          shouldShow = true; // Show product if no ranges are selected
        } else {
          selectedRanges.forEach(function(range) {
            var [min, max] = range.split('-').map(parseFloat);
            if (itemPrice >= min && itemPrice <= max) {
              shouldShow = true; // Show product if it falls within any selected range
            }
          });
        }

        if (shouldShow) {
          item.style.display = 'block';
        } else {
          item.style.display = 'none';
        }
      });

      // Update the price range display
      var filterPriceRange = document.getElementById('filter-price-range');
      filterPriceRange.textContent = selectedRanges.join(', ');
    }
  });
</script>


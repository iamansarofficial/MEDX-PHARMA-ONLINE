{{!-- TESTING OUT OF STOCK --}}
<div class="products mb-3" id="productContainer">
 
  {{#each products}}
    <div class="product product-list product-item" data-price="{{this.Price}}" data-category="{{this.Category}}">
      <div class="row">
        <div class="col-6 col-lg-3">
          <figure class="product-media">
            <span class="product-label label-new">New</span>
            <a href="product.html">
              <img src="/product-images/{{this._id}}.jpeg">
            </a>
          </figure><!-- End .product-media -->
        </div><!-- End .col-6 col-lg-3 -->

        <div class="col-6 col-lg-3 order-lg-last">
          <div class="product-list-action">
            <div class="product-price">
              {{this.Price}}
            </div><!-- End .product-price -->

            <div class="product-action">
              {{#if STOCK}}
                <a href="/detail-view/{{this._id}}" class="btn-product btn-quickview" title="Quick view"><span>quick view</span></a>
                <a href="#" class="btn-product btn-compare" title="Compare"><span>compare</span></a>
              {{/if}}


            </div><!-- End .product-action -->
              {{#if STOCK}}
                <a href="/detail-view/{{this._id}}" class="btn-product btn-cart"><span>buy now</span></a>
              {{else}}
                <h4 class="out-of-stock">Out of Stock</h4>
              {{/if}}
          </div><!-- End .product-list-action -->
        </div><!-- End .col-6 col-lg-3 -->

        <div class="col-lg-6">
          <div class="product-body product-action-inner">
            <a href="#" class="btn-product btn-wishlist" title="Add to wishlist"><span>add to wishlist</span></a>
            <div class="product-cat">
              <a href="#">{{this.Category}}</a>
            </div><!-- End .product-cat -->
            <h3 class="product-title"><a href="product.html">{{this.Name}}</a></h3><!-- End .product-title -->

            <div class="product-content">
              <p>{{this.Description}}</p>
            </div><!-- End .product-content -->

            <div class="product-nav product-nav-thumbs">
              <a href="#" class="active">
                <img src="/product-images/{{this._id}}.jpeg">
              </a>
              <a href="#">
                <img src="/product-images/{{this._id}}.jpeg">
              </a>
              <a href="#">
                <img src="/product-images/{{this._id}}.jpeg">
              </a>
            </div><!-- End .product-nav -->

          </div><!-- End .product-body -->
        </div><!-- End .col-lg-6 -->
     </div><!-- End .row -->
    </div><!-- End .product -->
  {{/each}}
</div><!-- End .products -->






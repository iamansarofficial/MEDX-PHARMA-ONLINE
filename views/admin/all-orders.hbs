    <link rel="stylesheet" href="/product-list/asset/css/plugins/nouislider/nouislider.css">
    <link rel="stylesheet" href="/product-list/asset/css/extra.css">




</head>

<body >
     
            <div class="header-middle sticky-header">
                <div class="container">
                    <div class="header-left">
                        <button class="mobile-menu-toggler">
                            <span class="sr-only">Toggle mobile menu</span>
                            <i class="icon-bars"></i>
                        </button>

                        <a href="index.html" class="logo" style="margin-top: 4.55rem;">
                            <img src="/frontHome/assets/img/logo 4.png" alt="" width="105" height="25">
                        </a>

                        <nav class="main-nav" style="margin-left: 51rem;">
                            <ul class="menu sf-arrows">
                                <li class="">
                                    <a href="/admin/" class="">Home</a>

                            
                                </li>
                                
                                <li>
                                    <a href="/admin/view-products" class="">Product</a>

                                    
                                </li>
                                <li >
                                    <a href="/admin/allUsers" class="">Users</a>

                                    
                                </li>
                                 <li  class="megamenu-container active">
                                    <a href="/admin/allOrders" class="">Orders</a>

                                    
                                </li>
                            </ul><!-- End .menu -->
                        </nav><!-- End .main-nav -->
                    </div><!-- End .header-left -->

                            <div class="dropdown-menu dropdown-menu-right">
                                <div class="dropdown-cart-products">
                                    <div class="product">
                                        <div class="product-cart-details">
                                            <h4 class="product-title">
                                                <a href="product.html">Beige knitted elastic runner shoes</a>
                                            </h4>

                                            <span class="cart-product-info">
                                                <span class="cart-product-qty">1</span>
                                                x $84.00
                                            </span>
                                        </div><!-- End .product-cart-details -->

                                        <figure class="product-image-container">
                                            <a href="product.html" class="product-image">
                                                <img src="/product-list/asset/images/products/cart/product-1.jpg" alt="product">
                                            </a>
                                        </figure>
                                        <a href="#" class="btn-remove" title="Remove Product"><i class="icon-close"></i></a>
                                    </div><!-- End .product -->

                                    <div class="product">
                                        <div class="product-cart-details">
                                            <h4 class="product-title">
                                                <a href="product.html">Blue utility pinafore denim dress</a>
                                            </h4>

                                            <span class="cart-product-info">
                                                <span class="cart-product-qty">1</span>
                                                x $76.00
                                            </span>
                                        </div><!-- End .product-cart-details -->

                                        <figure class="product-image-container">
                                            <a href="product.html" class="product-image">
                                                <img src="/product-list/asset/images/products/cart/product-2.jpg" alt="product">
                                            </a>
                                        </figure>
                                        <a href="#" class="btn-remove" title="Remove Product"><i class="icon-close"></i></a>
                                    </div><!-- End .product -->
                                </div><!-- End .cart-product -->

                                <div class="dropdown-cart-total">
                                    <span>Total</span>

                                    <span class="cart-total-price">$160.00</span>
                                </div><!-- End .dropdown-cart-total -->

                                <div class="dropdown-cart-action">
                                    <a href="cart.html" class="btn btn-primary">View Cart</a>
                                    <a href="checkout.html" class="btn btn-outline-primary-2"><span>Checkout</span><i class="icon-long-arrow-right"></i></a>
                                </div><!-- End .dropdown-cart-total -->
                            </div><!-- End .dropdown-menu -->
                        </div><!-- End .cart-dropdown -->
                    </div><!-- End .header-right -->
                </div><!-- End .container -->
            </div><!-- End .header-middle -->
<section>
     <h3 class="tittle text-center" style="color: #c96; margin-top: 3rem;">All Orders</h3>
    <div class="container fadeInDown third" style="margin-top: 4rem;" >
        <table class="table mt-5" id="order-listing" >
            <thead class="tableHead">
                <tr>
                    <th scope="col">Date</th>
                    <th>Address</th>
                    <th>Pincode</th>
                    <th>Mobile</th>
                    <th scope="col">Amount</th>
                    <th scope="col">Payment</th>
                    <th>Status</th>
                    <th>View Products</th>
                  
                </tr>
            </thead>
            <tbody style="color: white;">
                {{#each orders}}
                    <tr>
                        <td  style="font-size: 1rem;">{{this.date}}</td>
                        <td  style="font-size: 1rem;">{{this.deliveryDetails.address}}</td>
                        <td  style="font-size: 1rem;">{{this.deliveryDetails.pincode}}</td>
                        <td  style="font-size: 1rem;">{{this.deliveryDetails.mobile}}</td>
                        <td  style="font-size: 1.3rem;">â‚¹{{this.totalAmount}}.00</td>
                        <td  style="font-size: 1.3rem;">{{this.paymentMethod}}</td>
                        <td  style="font-size: 1.3rem;">{{this.status}}</td>
                        <td>
                            {{!-- <input type="hidden" name="orderId" value="{{orderId}}"> --}}
                            <a href="/admin/viewUserOrderProducts/{{this._id}}" class="btn btn-primary cartButton">View Products</a>
                        </td>

                    </tr>
                {{/each}}
            </tbody>
        </table>

    </div>
</section>


<section>
<section>
  <div class="container fadeInDown third">
    <table class="table mt-5">
      <thead class="tableHead">
        <tr>
          <th>Name</th>
          <th>Email</th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <td style=" font-size: 18px;">{{user.username}}</td>
          <td style=" font-size: 18px;">{{user.email}}</td>
        </tr>
      </tbody>
    </table>
  </div>
</section>






  <form action="/admin/viewUserOrderProducts/{{orderId}}" method="POST">
    <div class="container mt-5">
      <div class="row">
        {{#each products}}
        <div class="col-md-3 p-3">
          <div class="card" style="width: 15rem; height: 24rem;">
            <img class="card-img-top" src="/product-images/{{this.product._id}}.jpeg" alt="card image cap" style="height: 12rem;">
            <div class="card-body">
              <h5 class="card-title" style="color: #68a952;">{{this.product.Name}}</h5>
              <p class="card-text">â‚¹{{this.product.Price}}</p>
              <p class="card-text">Quantity: {{this.quantity}}</p>
              <p class="card-text">{{this.description}}</p>
              <a href="/detail-view/{{this.product._id}}"><button class="btn btn-success">View product</button></a>
            </div>
          </div>
        </div>
        {{/each}}
      </div>
    </div>

    <div class="container mt-5">
    <div class="row align-items-center">
      <div class="col-md-6">
       <div class="btn-group">
  <button type="button" style=" margin-left: 38rem;" class="btn btn-outline-primary-2 btn-order dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
    Update Status
  </button>
  <div class="dropdown-menu">
    {{#if orderReturn}}
    <a class="dropdown-item" href="/admin/changeOrderStatus/{{orderId}}/Return-Accepted">Return Accepted</a>
{{else}}
    <a class="dropdown-item" href="/admin/changeOrderStatus/{{orderId}}/delay">Delay</a>
    <a class="dropdown-item" href="/admin/changeOrderStatus/{{orderId}}/delivered">Delivered</a>
    {{/if}}
  </div>
</div>

      </div>
      <div class="col-md-6">
        <input type="hidden" name="orderId" value="{{orderId}}">
        <button type="submit" class="btn btn-outline-primary-2 btn-order btn-block">
          <span class="btn-text">Remove Order</span>
          <span class="btn-hover-text">Click to Remove</span>
        </button>
      </div>
    </div>
  </div>
</form>
</section>
    